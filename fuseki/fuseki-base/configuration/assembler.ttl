@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tdb:     <http://jena.hpl.hp.com/2008/tdb#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix tdb2:    <http://jena.apache.org/2016/tdb#> .
@prefix text:    <http://jena.apache.org/text#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix skosxl:  <http://www.w3.org/2008/05/skos-xl#> .
@prefix agent:   <http://schema.onki.fi/agent-schema#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix spatial: <http://jena.apache.org/spatial#> .
@prefix schema:  <http://schema.org/> .
@prefix :        <#> .


<#kbpedia-service> rdf:type fuseki:Service ;
    fuseki:name              "kbpedia" ;       # http://host:port/kbpedia
    fuseki:serviceQuery      "sparql" ;           # SPARQL query service
    fuseki:serviceUpdate     "update" ;           # SPARQL update service
    fuseki:serviceUpload     "upload" ;           # Non-SPARQL upload service
    fuseki:serviceReadWriteGraphStore "data" ;    # SPARQL Graph store protocol
    fuseki:dataset           <#kbpedia-dataset> ;
    .

<#kbpedia-dataset> rdf:type tdb:DatasetTDB2 ;
    rdf:type ja:RDFDataset;
    tdb:location "/fuseki-base/databases/kbpedia" ;
    # Query timeout on this dataset (1s, 1000 milliseconds)
    #ja:context [ ja:cxtName "arq:queryTimeout" ;  ja:cxtValue "30000" ] ;
     .


<#wordnet-service> rdf:type fuseki:Service ;
    fuseki:name              "wordnet" ;       # http://host:port/wordnet
    fuseki:serviceQuery      "sparql" ;           # SPARQL query service
    fuseki:serviceUpdate     "update" ;           # SPARQL update service
    fuseki:serviceUpload     "upload" ;           # Non-SPARQL upload service
    fuseki:serviceReadWriteGraphStore "data" ;    # SPARQL Graph store protocol
    fuseki:dataset           <#wordnet-dataset> ;
    .

<#wordnet-dataset> rdf:type tdb:DatasetTDB2 ;
    rdf:type ja:RDFDataset;
    tdb:location "/fuseki-base/databases/wordnet" ;
    # Query timeout on this dataset (1s, 1000 milliseconds)
    #ja:context [ ja:cxtName "arq:queryTimeout" ;  ja:cxtValue "30000" ] ;
     .

