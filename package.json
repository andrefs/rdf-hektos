{
  "name": "rdf-props-summarizer",
  "version": "0.0.1",
  "description": "Extract a property summary from an RDF graph",
  "scripts": {
    "typecheck": "tsc -b tsconfig.json",
    "test": "vitest",
    "build": "tsc",
    "postbuild": "cp package.json build/package.json && cp yarn.lock build/yarn.lock && cd build && yarn install",
    "start": "time node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "proc-graph": "time node --experimental-specifier-resolution=node --loader ts-node/esm ./src/bin/proc-graph.ts",
    "graph-to-csv": "time node --experimental-specifier-resolution=node --loader ts-node/esm ./src/bin/sum2csv.ts"
  },
  "keywords": [
    "RDF",
    "graph",
    "GraphDB",
    "SPARQL"
  ],
  "author": "Andr√© Santos",
  "license": "ISC",
  "dependencies": {
    "@comunica/logger-pretty": "1.22.0",
    "@comunica/query-sparql": "2.0.5",
    "@comunica/types": "2.0.1",
    "@rdfjs/data-model": "2.0.0",
    "@rdfjs/namespace": "2.0.0",
    "@rdfjs/types": "1.1.0",
    "@tsconfig/node22": "22.0.0",
    "@types/bluebird-global": "3.5.18",
    "@types/cli-progress": "3.11.6",
    "@types/rdfjs__data-model": "2.0.1",
    "bluebird": "3.7.2",
    "cli-progress": "3.12.0",
    "rdf-data-factory": "1.1.0",
    "sparql-http-client": "2.4.0",
    "sparqljs": "3.7.3",
    "ts-command-line-args": "2.5.1",
    "vitest": "2.1.4"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "n3": "1.13.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  }
}
